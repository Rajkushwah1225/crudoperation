<h1>New Product</h1>

<%= form_with model: @products do |form| %>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :price %><br>
    <%= form.text_area :price %>
  </div>
   <div>
    <%= form.label :credit %><br>
    <%= form.text_area :credit%>
  </div>
  <div>
    <%= form.label :debit %><br>
    <%= form.text_area :debit %>
  </div>


  <div>
    <%= form.submit %>
  </div>
<% end %>
