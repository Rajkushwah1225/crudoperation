
<div class="card mt-5 mb-5">
<div class="card-body">
<h3 class="text-center"> <%= t('listing_employees') %> </h3>
<%= link_to "Create New Employee", employees_new_path %>
<%=flash[:notice]= "employee created...."%>
<table>
<thead>
<style>
  td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
<tr>
   <th>first_name</th>
   <th>last_name</th>
   <th>Email</th>
   <th>city</th>
   <th>state</th>
   <th>country</th>
   <th> Pincode </th>
   <th>Delete</th>
</tr>
</thead>
<tbody class="body">
<tr>
<%= @employees.each do |employee|%>
  <td><%= employee.first_name%></td>
  <td><%= employee.last_name%></td>
  <td><%= employee.email%></td>
  <td><%= employee.city%></td>
  <td><%= employee.state%></td>
  <td><%= employee.country%></td>
  <td><%= employee.pincode%></td>
  <td><%= link_to "Delete", employees_edit_path %></td>
</tr>
<%= end %>
</tbody>
</table>
